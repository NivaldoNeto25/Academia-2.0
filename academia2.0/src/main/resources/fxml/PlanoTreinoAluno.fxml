<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="br.upe.academia2.ui.controllers.PlanoTreinoAlunoController"
            prefWidth="600" prefHeight="400">
    <Label layoutX="220" layoutY="40" text="Plano de Treino" style="-fx-font-size: 24"/>
    <Button layoutX="220" layoutY="100" prefWidth="200" text="Cadastrar Plano de Treino"
            onAction="#handleCadastrarPlano"/>
    <Button layoutX="220" layoutY="150" prefWidth="200" text="Listar Plano de Treino" onAction="#handleListarPlano"/>
    <Button layoutX="220" layoutY="200" prefWidth="200" text="Modificar Plano de Treino"
            onAction="#handleModificarPlano"/>
    <Button layoutX="220" layoutY="250" prefWidth="200" text="Seção Treino" onAction="#handleSecaoTreino"/>
    <Button fx:id="btnVoltar" layoutX="40" layoutY="350" prefWidth="100" text="Voltar" onAction="#handleVoltar"/>
</AnchorPane>
